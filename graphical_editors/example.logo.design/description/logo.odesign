        </nodeMappings>
        <edgeMappings name="NextInstructionEdge" sourceMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='ControlStructureInstructionNode']" targetMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='ControlStructureInstructionNode']" targetFinderExpression="service:getNextInstruction()">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="CalledProcEdge" sourceMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode']" targetMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='ProcDeclarationContainer']" targetFinderExpression="feature:declaration">
          <style lineStyle="dash" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="ProcDeclarationContainer" semanticCandidatesExpression="feature:eContents" domainClass="logoASM.ProcDeclaration" reusedNodeMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode']">
          <subContainerMappings xsi:type="description_1:ContainerMappingImport" reusedContainerMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='BlockNode']" importedMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='ControlStructureInstructionNode']"/>
          <style xsi:type="style:FlatContainerStyleDescription">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </containerMappings>
        <containerMappings name="ControlStructureInstructionNode" semanticCandidatesExpression="feature:eContents" domainClass="logoASM.ControlStructure" reusedNodeMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode']" reusedContainerMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='BlockNode']">
          <subContainerMappings xsi:type="description_1:ContainerMappingImport" name="ControlStructureContainerImport" reusedNodeMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode']" reusedContainerMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='BlockNode']" importedMapping="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@containerMappings[name='ControlStructureInstructionNode']"/>
          <style xsi:type="style:FlatContainerStyleDescription" labelExpression="service:getLabelForControlStructure()">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </containerMappings>
        <containerMappings name="BlockNode" semanticCandidatesExpression="feature:eContents" domainClass="logoASM.Block" reusedNodeMappings="//@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='PrimitiveInstructionNode'] //@ownedViewpoints[name='Logo']/@ownedRepresentations[name='Logo']/@defaultLayer/@nodeMappings[name='ProcCallNode']">
          <style xsi:type="style:FlatContainerStyleDescription" labelExpression="service:getLabelForInstruction()">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
          </style>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="example.logo.design.services.LogoDiagramServices"/>
  </ownedViewpoints>
</description:Group>
